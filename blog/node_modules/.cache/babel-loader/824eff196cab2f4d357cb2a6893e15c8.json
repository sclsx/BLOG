{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: 'MessagePage'\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;AAuBA;EACAA;AADA","names":["name"],"sourceRoot":"src/components","sources":["MessagePage.vue"],"sourcesContent":["<template>\r\n    <div id=\"Message\">\r\n        <article v-for=\"item in articleData\" :key=\"item._id\" class=\"infinite-list-item\">\r\n            <h2>{{item.title}}</h2>\r\n            <div class=\"time\">\r\n                <p class=\"date\">{{new Date(item.date).getDate()}}</p>\r\n                <p class=\"m-y\">\r\n                    <span>{{new Date(item.date).getMonth()+1}}</span>\r\n                    <span>{{new Date(item.date).getFullYear()}}</span>\r\n                </p>\r\n            </div>\r\n            <div class=\"show\">\r\n                <div class=\"img\" :style=\"{backgroundImage: `url(${baseURL}${item.cover})`}\"></div>\r\n                <div class=\"des\">{{item.des}}</div>\r\n            </div>\r\n            <div class=\"r-b\">\r\n                <router-link :to=\"`/article/${item._id}`\">阅读全文</router-link>\r\n            </div>\r\n        </article>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'MessagePage'\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}