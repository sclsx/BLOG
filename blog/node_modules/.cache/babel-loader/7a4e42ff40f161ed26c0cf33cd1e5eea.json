["name","data","ruleForm","email","subject","message","rules","required","trigger","hotArticle","methods","submitForm","formName","valid","method","url","getArticle","created"],"sourceRoot":"src/components","sources":["HomePage.vue"],"sourcesContent":["<template>\r\n    <div id=\"Home\">\r\n        <!-- 个人信息部分 -->\r\n        <article class=\"hello\">\r\n        <h2>HELLO!</h2>\r\n        <p class=\"des\">\r\n            你好！欢迎来到我的博客！我是一名大三的学生，喜欢做自己的网页，热衷于研究JavaScript、NodeJS、Vue。这是我的主页！我会在这里发表一些不成熟的技术文章，记录一些日常与我的看法，博客目前使用的是Vue搭建的，如果对源码感兴趣的话可以<a href=\"https://github.com/sclsx/test\" target=\"_blank\">点击这里</a>提出宝贵的建议。</p>\r\n        <div class=\"info\">\r\n            <div class=\"personal\">\r\n                <h3>personal information</h3>\r\n                <ul>\r\n                    <li>\r\n                        <p>NICKNAME</p>\r\n                        <p>Carol</p>\r\n                    </li>\r\n                    <li>\r\n                        <p>ADDRESS</p>\r\n                        <p>翻斗花园2号楼1001室</p>\r\n                    </li>\r\n                    <li>\r\n                        <p>EMAIL</p>\r\n                        <p>52331314@qq.com</p>\r\n                    </li>\r\n                    <li>\r\n                        <p>GITHUB</p>\r\n                        <p><a href=\"https://github.com/\" target=\"_blank\">https://github.com/</a></p>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n            <div class=\"skills\">\r\n                <h3>skills</h3>\r\n                <ul>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>HTML5 & CSS3</span>\r\n                            <span>90%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 90%\"></span>\r\n                        </p>\r\n                    </li>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>Javascript</span>\r\n                            <span>85%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 85%\"></span>\r\n                        </p>\r\n                    </li>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>Vue</span>\r\n                            <span>80%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 80%\"></span>\r\n                        </p>\r\n                    </li>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>NodeJS</span>\r\n                            <span>70%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 70%\"></span>\r\n                        </p>\r\n                    </li>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>AXI</span>\r\n                            <span>50%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 50%\"></span>\r\n                        </p>\r\n                    </li>\r\n                    <li>\r\n                        <p class=\"lang\">\r\n                            <span>Electron</span>\r\n                            <span>50%</span>\r\n                        </p>\r\n                        <p class=\"level\">\r\n                            <span style=\"width: 50%\"></span>\r\n                        </p>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n        </article>\r\n        <!-- 文章部分 -->\r\n        <article class=\"article\">\r\n            <h2>ARTICLE</h2>\r\n            <p class=\"des\">热门文章推荐。<router-link to=\"/article\">更多文章</router-link>请访问文章页</p>\r\n            <div class=\"content\" v-if=\"hotArticle.length\">\r\n                <div class=\"left_article\">\r\n                    <div class=\"art\">\r\n                        <img src=\"../img/1.jpg\">\r\n                        <p>{{hotArticle[0].title}}</p>\r\n                        <div class=\"cover\">\r\n                            <router-link :to=\"`/article/${hotArticle[0]._id}`\" target=\"_blank\">\r\n                                <p>Tag: HTML</p>\r\n                                <p>View More</p>\r\n                            </router-link>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"art\">\r\n                        <img src=\"../img/3.jpg\">\r\n                        <p>{{hotArticle[1].title}}</p>\r\n                        <div class=\"cover\">\r\n                            <router-link :to=\"`/article/${hotArticle[1]._id}`\" target=\"_blank\">\r\n                                <p>Tag: HTML</p>\r\n                                <p>View More</p>\r\n                            </router-link>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"right_article\">\r\n                    <div class=\"art\">\r\n                        <img src=\"../img/2.jpg\">\r\n                        <p>{{hotArticle[2].title}}</p>\r\n                        <div class=\"cover\">\r\n                            <router-link :to=\"`/article/${hotArticle[2]._id}`\" target=\"_blank\">\r\n                                <p>Tag: HTML</p>\r\n                                <p>View More</p>\r\n                            </router-link>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </article>\r\n        <!-- work部分 -->\r\n        <article class=\"work\">\r\n          <h2>WORK EXPERIENCE</h2>\r\n          <ul>\r\n            <li>\r\n              <div class=\"left_work\">\r\n                <p class=\"time\">2020-</p>\r\n              </div>\r\n              <div class=\"right_work\">\r\n                <h3>WEB DEVELOPER/ONLINE TEACHER</h3>\r\n                <p class=\"img\">\r\n                  <img src=\"../img/html.jpg\">\r\n                </p>\r\n                <p class=\"company\">Zzt EDU</p>\r\n                <p class=\"content\">Responsible for the development of independent official website of some subjects of the\r\n              company.And also as an online teacher of WEB.Technology consists of HTML5, CSS3, JavaScript, Nodejs, vue,\r\n              Canvas, Design pattern & Algorithm。</p>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div class=\"left_work\">\r\n                <p class=\"time\">2020-</p>\r\n              </div>\r\n              <div class=\"right_work\">\r\n                <h3>WEB DEVELOPER/ONLINE TEACHER</h3>\r\n                <p class=\"img\">\r\n                  <img src=\"../img/html.jpg\">\r\n                </p>\r\n                <p class=\"company\">Zzt EDU</p>\r\n                <p class=\"content\">Responsible for the development of independent official website of some subjects of the\r\n              company.And also as an online teacher of WEB.Technology consists of HTML5, CSS3, JavaScript, Nodejs, vue,\r\n              Canvas, Design pattern & Algorithm。</p>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div class=\"left_work\">\r\n                <p class=\"time\">2020-</p>\r\n              </div>\r\n              <div class=\"right_work\">\r\n                <h3>WEB DEVELOPER/ONLINE TEACHER</h3>\r\n                <p class=\"img\">\r\n                  <img src=\"../img/html.jpg\">\r\n                </p>\r\n                <p class=\"company\">Zzt EDU</p>\r\n                <p class=\"content\">Responsible for the development of independent official website of some subjects of the\r\n              company.And also as an online teacher of WEB.Technology consists of HTML5, CSS3, JavaScript, Nodejs, vue,\r\n              Canvas, Design pattern & Algorithm。</p>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div class=\"left_work\">\r\n                <p class=\"time\">2020-</p>\r\n              </div>\r\n              <div class=\"right_work\">\r\n                <h3>WEB DEVELOPER/ONLINE TEACHER</h3>\r\n                <p class=\"img\">\r\n                  <img src=\"../img/html.jpg\">\r\n                </p>\r\n                <p class=\"company\">Zzt EDU</p>\r\n                <p class=\"content\">Responsible for the development of independent official website of some subjects of the\r\n              company.And also as an online teacher of WEB.Technology consists of HTML5, CSS3, JavaScript, Nodejs, vue,\r\n              Canvas, Design pattern & Algorithm。</p>\r\n              </div>\r\n            </li>\r\n          </ul>\r\n        </article>\r\n        <!-- contact -->\r\n        <article class=\"contact\" id=\"contact\">\r\n          <h2>CONTACT ME</h2>\r\n          <p class=\"des\">有任何问题请联系我吧，收到消息后会给您回复邮件哦</p>\r\n          <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" class=\"demo-ruleForm\">\r\n            <el-form-item label=\"Your Name\" prop=\"name\">\r\n              <el-input v-model=\"ruleForm.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"Your Email\" prop=\"email\">\r\n              <el-input v-model=\"ruleForm.email\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"Subject\" prop=\"subject\">\r\n              <el-input v-model=\"ruleForm.subject\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"Your Message\" prop=\"message\">\r\n              <el-input v-model=\"ruleForm.message\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">Send</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </article>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'HomePage',\r\n    data() {\r\n      return {\r\n        ruleForm: {\r\n          name: '',\r\n          email: '',\r\n          subject: '',\r\n          message: ''\r\n        },\r\n        rules: {\r\n          name: [\r\n            { required: true, message: '请输入活动名称', trigger: 'blur' }\r\n          ],\r\n          email: [\r\n            { required: true, message: '请输入邮箱', trigger: 'blur' }\r\n          ],\r\n          message: [\r\n            { required: true, message: '请输入信息', trigger: 'blur' }\r\n          ]\r\n        },\r\n        hotArticle: []\r\n      };\r\n    },\r\n    methods: {\r\n      submitForm(formName) {\r\n        this.$refs[formName].validate(async (valid) => {\r\n          if (valid) {\r\n            let {data} = await this.$axios({\r\n              method: 'POST',\r\n              url: '/contact',\r\n              data: this.ruleForm\r\n            })\r\n            if (data.code) {\r\n              return this.$message.error(data.msg)\r\n            }\r\n            this.$message.success('发送成功')\r\n            this.$refs[formName].resetFields()\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      async getArticle() {\r\n          let {data} = await this.$axios({\r\n              method: 'GET',\r\n              url: '/get/article'\r\n          })\r\n          if(data.code) return\r\n          this.hotArticle = data.data.slice(0,3)\r\n      }\r\n    },\r\n    created() {\r\n      this.getArticle()\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#Home {\r\n  article {\r\n    background-color: #fff;\r\n    padding: 15px;\r\n    margin-bottom: 15px;\r\n    font-fam